var solUtils={};$svjq.fn.changeElementType=function(b){var a={};$svjq.each(this[0].attributes,function(b,c){a[c.nodeName]=c.nodeValue});this.replaceWith(function(){return $svjq("\x3c"+b+"/\x3e",a).append($svjq(this).contents())})};solUtils.clickableElement=function(b){solUtils.isOnline()&&$svjq(b).each(function(){var a=$svjq(this).find("a").first();0<a.length&&($svjq(this).wrap($svjq(a).clone().html("")),$svjq(a).removeAttr("title").removeAttr("href").changeElementType("span"))})};
solUtils.isOnline=function(){return 0===$svjq("html.sv-edit-mode").length};$svjq(document).ready(function(){solUtils.clickableElement(".clickableElement")});
